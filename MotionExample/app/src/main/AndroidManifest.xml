<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.motionexample">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MotionExampleApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MotionExample">
        <activity
            android:name=".RetrofitActivity"
            android:exported="false" />
        <activity
            android:name=".BdActivity"
            android:exported="false" />

        <receiver
            android:name=".MessageReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="cat" />
            </intent-filter>
        </receiver>

        <service
            android:name=".MediaCatService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".HelloDrawer"
            android:exported="false" />
        <activity android:name=".HelloMotion" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>